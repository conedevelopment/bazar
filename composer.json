{
    "name": "conedevelopment/bazar",
    "description": "Bazar is a drop-in package for e-commerce Laravel applications.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Cone Development",
            "email": "hello@conedevelopment.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bazar\\": "src/",
            "Bazar\\Database\\Seeders\\": "database/seeders/",
            "Bazar\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bazar\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "laravel/laravel": "^8.0",
        "laravel/ui": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bazar\\BazarServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
