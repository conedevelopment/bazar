{
    "name": "conedevelopment/bazar",
    "description": "Bazar is a drop-in package for e-commerce Laravel applications.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Cone Development",
            "email": "hello@conedevelopment.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cone\\Bazar\\": "src/",
            "Cone\\Bazar\\Database\\Seeders\\": "database/seeders/",
            "Cone\\Bazar\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cone\\Bazar\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3.0 || ^8.4.0",
        "conedevelopment/root": "^2.6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/laravel": "^12.0",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^11.5",
        "laravel/pint": "^1.6",
        "larastan/larastan": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cone\\Bazar\\BazarServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
